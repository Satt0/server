extends layout
block content
    //- a(class="return-btn" href="javascript:history.back()") return 
    -var temp=ok    
    div !{temp}
    div(class="player-select")
        h4 choose server
        for item in player
           button(id=item.player onclick=`goto1("`+item.player+`")`)=item.player
    div(class="eps-select")
        h4 choose episode
        for item in eps
            button(onclick=`goto("`+item.episode+`");`)=item.episode
    script
        include ./dist/js/player.js